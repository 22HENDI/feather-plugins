<% throw_content :right do %>
  <p>
	Setup a new release here.
  </p>
<% end %>

<%= error_messages_for @release %>
<h1>New Release</h1>

<%= form_for :release, :action => url(:admin_release, @release) do %>
  <%= partial 'form' %>
  <p><%= submit 'Save Release' %> or <%= link_to 'Cancel', url(:admin_releases) %></p>
<% end =%>

<%= link_to "Back to releases", url(:admin_releases) %>
